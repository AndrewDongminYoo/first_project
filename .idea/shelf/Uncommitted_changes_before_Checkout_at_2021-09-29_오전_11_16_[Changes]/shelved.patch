<+>/*Bulma CSS 약간의 수정 들어간 것*/\r\n@import \"bulma.css\";\r\n/*배달 점포 카드 애니메이션*/\r\n@import \"card.css\";\r\n/*스크롤업 버튼*/\r\n@import \"scroll_up.css\";\r\n\r\n* {\r\n    font-family: 'Hanna', sans-serif;\r\n}\r\n\r\nbody {\r\n    width: 100%;\r\n    height: 100vh;\r\n    margin: 0 auto;\r\n    grid-template-columns: repeat(16, 1fr);\r\n    grid-template-rows: repeat(8, 1fr);\r\n    box-sizing: border-box !important;\r\n}\r\n/* 여기부터 헤더 히어로 */\r\n.header {\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    align-items: center;\r\n    font-size: 24px;\r\n    height: 250px;\r\n    background-color: #ffebcd;\r\n    background-size: cover;\r\n    background-repeat: repeat-x;\r\n    background-image: url(\"../images/lucian-alexe.jpg\");\r\n}\r\n\r\n.header > h1.title  {\r\n    font-family: 'Black Han Sans', sans-serif;\r\n    text-shadow: 4px 4px 0 saddlebrown;\r\n    font-size: 70px;\r\n    color: burlywood;\r\n    margin-bottom: 30px;\r\n}\r\n\r\n/*.name {*/\r\n/*    font-family: \"Roboto Slab\", serif;*/\r\n/*    font-weight: bold;*/\r\n/*    font-size: 50px;*/\r\n/*    padding: 20px;*/\r\n/*}*/\r\n\r\ndiv.container {\r\n    padding: 20px;\r\n    flex-grow: 1;\r\n    margin: 0 auto;\r\n    position: relative;\r\n    width: auto;\r\n    /*max-width: 1200px !important;*/\r\n}\r\n/*검색 인풋*/\r\ndiv.control {\r\n    min-width: 200px;\r\n    width: 300px;\r\n    display: inline-flex;\r\n}\r\n/*카드 컬럼 부분*/\r\n.columns.is-max-desktop.is-mobile\r\n.columns:not(.is-desktop) {\r\n    display: flex;\r\n    max-width: 1200px;\r\n    margin: auto;\r\n}\r\n/*버튼 부분*/\r\nbutton.button {\r\n    -moz-appearance: none;\r\n    -webkit-appearance: none;\r\n    align-items: center;\r\n    border: 1px solid transparent;\r\n    box-shadow: none;\r\n    display: inline-flex;\r\n    justify-content: flex-start;\r\n    line-height: 1.5;\r\n    position: relative;\r\n    vertical-align: top;\r\n}\r\n\r\ndiv.buttons {\r\n    align-items: center;\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    justify-content: center;\r\n    margin-bottom: unset;\r\n}\r\n\r\n.button.is-warning {\r\n    background-color: #ffe08a;\r\n    cursor: pointer;\r\n    justify-content: center;\r\n    text-align: center;\r\n    white-space: nowrap;\r\n    margin-right: .5rem;\r\n}\r\n\r\n.button {\r\n    margin-right: 10px;\r\n}\r\n/*버튼 끝*/\r\n/*배경 이미지 현재 미적용 중*/\r\n/*.bg-img {*/\r\n/*    width: 100vw;*/\r\n/*    height: 100vh;*/\r\n/*    overflow: hidden;*/\r\n/*}*/\r\n\r\n/*여기서부터 즐겨찾기목록 띄우는 CSS*/\r\nh2.h2 {\r\n    font-size: x-large;\r\n    font-weight: 500;\r\n}\r\n/*즐겨찾기 삭제버튼*/\r\nbutton.button.is-xs {\r\n    background-color: #00d1b2;\r\n    border-color: transparent;\r\n    color: #fff;\r\n    border-radius: 9999px;\r\n    font-size: 0.5rem;\r\n    height: 22px;\r\n}\r\n/*즐겨찾기 리스트*/\r\n.bookmark {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    margin-top: 4px;\r\n}\r\n/*즐겨찾기 리스트 텍스트*/\r\nspan.mark-menu {\r\n    width: 8rem;\r\n    overflow: hidden;\r\n    height: 1rem;\r\n    display: inline-block !important;\r\n}\r\n\r\n/*즐겨찾기 목록 부드럽게 오른쪽에서 슬라이드로 나타나는 효과 + 스크롤따라 움직임*/\r\n#aside {\r\n    font-size: 14px;\r\n    z-index: 100;\r\n    -webkit-transform: translate3d(300px, 0, 0);\r\n    -moz-transform: translate3d(300px, 0, 0);\r\n    transform: translate3d(300px, 0, 0);\r\n    -webkit-transition: all 0.2s;\r\n    -moz-transition: all 0.2s;\r\n    -ms-transition: all 0.2s;\r\n    -o-transition: all 0.2s;\r\n    transition: all 0.2s;\r\n}\r\n\r\n#aside.open {\r\n    overflow-y: auto;\r\n    -webkit-transform: translate3d(0, 0, 0);\r\n    -moz-transform: translate3d(0, 0, 0);\r\n    transform: translate3d(0, 0, 0);\r\n    -webkit-transition: all 0.2s;\r\n    -moz-transition: all 0.2s;\r\n    -ms-transition: all 0.2s;\r\n    -o-transition: all 0.2s;\r\n    transition: all 0.2s;\r\n}\r\n\r\n#aside {\r\n    padding: 10px;\r\n    background-color: #cfcfcf;\r\n    border-radius: 10px;\r\n    position: sticky;\r\n    top: 100px;\r\n    right: 300px;\r\n}\r\n
